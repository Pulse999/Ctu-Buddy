{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "thank-you.html",
      "use": "@vercel/static"
    },
    {
      "src": "thanks.css",
      "use": "@vercel/static"
    },
    {
      "src": "Styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "Contact-styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "Contact.html",
      "use": "@vercel/static"
    },
    {
      "src": "AboutUs.html",
      "use": "@vercel/static"
    },
    {
      "src": "AboutUs.css",
      "use": "@vercel/static"
    },
    {
      "src": "Timetable.html",
      "use": "@vercel/static"
    },
    {
      "src": "Timetable.css",
      "use": "@vercel/static"
    },
    {
      "src": "Discussions.html",
      "use": "@vercel/static"
    },
    {
      "src": "Discussions.css",
      "use": "@vercel/static"
    },
    {
      "src": "Recources.html",
      "use": "@vercel/static"
    },
    {
      "src": "Rescources-styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "Recources.js",
      "use": "@vercel/node"
    },
    {
      "src": "reset.css",
      "use": "@vercel/static"
    },
    {
      "src": "calender_script.js",
      "use": "@vercel/node"
    },
    {
      "src": "AboutUs Img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Timetable Img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Uploads/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "fonts/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/submit-form",
      "dest": "/server.js"  
    },
    {
      "src": "/",
      "dest": "/index.html" 
    },
    {
      "src": "/thank-you",
      "dest": "/thank-you.html" 
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
